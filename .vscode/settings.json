{
    "svn.ignoreMissingSvnWarning": true,
    "java.jdt.ls.java.home": "/usr/lib/jvm/java-21-openjdk-amd64",
    "java.compile.nullAnalysis.mode": "automatic",
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.compile.nullAnalysis.nonnull": [],
    "java.compile.nullAnalysis.nullable": [],
    "java.checkstyle.version": "10.17.0",
    "java.checkstyle.configuration": "${workspaceFolder}/config/checkstyle/google_checks.xml",
    "checkstyle.trace.server": "verbose",
    "checkstyle.configurationPath": "https://raw.githubusercontent.com/google/styleguide/gh-pages/google_checks.xml",
    "checkstyle.enable": true,
    "checkstyle.checkstylePath": "/usr/lib/jvm/java-21-openjdk-amd64/bin/java",
    "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
    "java.format.settings.profile": "GoogleStyle",
    "editor.formatOnSave": false,
    "[java]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "redhat.java"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.checkstyle": "explicit"
    }
